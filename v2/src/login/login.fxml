<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="420.0" prefWidth="419.0" stylesheets="@../resources/dark-theme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.LoginController">
   <children>
      <JFXTextField fx:id="username" labelFloat="true" layoutX="95.0" layoutY="188.0" prefHeight="33.0" prefWidth="259.0" promptText="Nom d'utilisateur">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="password" labelFloat="true" layoutX="97.0" layoutY="260.0" onAction="#handleLoginButtonAction" prefHeight="34.0" prefWidth="259.0" promptText="Mot de passe">
         <font>
            <Font size="17.0" />
         </font>
      </JFXPasswordField>
      <JFXButton layoutX="106.0" layoutY="344.0" onAction="#handleLoginButtonAction" prefHeight="46.0" prefWidth="101.0" styleClass="btn-default" text="OK" />
      <JFXButton layoutX="253.0" layoutY="344.0" onAction="#handleCancelButtonAction" prefHeight="46.0" prefWidth="101.0" styleClass="btn-default" text="Annuler" />
      <ImageView fitHeight="37.0" fitWidth="38.0" layoutX="45.0" layoutY="192.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/icon/gender_neutral_user_48px.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="105.0" fitWidth="135.0" layoutX="142.0" layoutY="25.0" pickOnBounds="true">
         <image>
            <Image url="@../resources/icon/lock_96px.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="37.0" fitWidth="38.0" layoutX="45.0" layoutY="261.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/icon/password_48px.png" />
         </image>
      </ImageView>
   </children>
   
</AnchorPane>
